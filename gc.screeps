module.exports = {
    gc: function() {
        for(var sid in Memory.structures) {
            if(!Game.structures[sid]) {
                delete Memory.structures[sid];
            }
            if(!Memory.structures[sid] || _.isEmpty(Memory.structures[sid])) {
                delete Memory.structures[sid];
            }
        }
        for(var name in Memory.creeps) {
            if(!Game.creeps[name]) {
                delete Memory.creeps[name];
            }
            if(!Memory.creeps[name] || _.isEmpty(Memory.creeps[name])) {
                delete Memory.creeps[name];
            }
        }
    }
}
